    <script>
            // 增肌增力
    const startButton = document.getElementById('pointAButton');
    const hexElement = document.getElementById('LineA');
    let animationRunning = false; // 动画是否正在运行的标志

    startButton.addEventListener('click', toggleAnimation);

    function toggleAnimation() {
        if (!animationRunning) {
            hexElement.classList.add('animateLine'); // 添加animate类以启动动画
            animationRunning = true;
        } else {
            hexElement.classList.remove('animateLine'); // 移除animate类以暂停动画


            hexElement.style.strokeDashoffset = '1000px'; // 将动画重置到起始状态

            // 使用setTimeout在下一帧重新执行动画
            setTimeout(() => {
                hexElement.classList.add('animateLine'); // 添加animate类以重新启动动画
            }, 0);
        }
    }
    // 塑形減脂
    const startCButton = document.getElementById('pointCButton');
    const hexElementC = document.getElementById('LineC');
    let animationRunningC = false; // 动画是否正在运行的标志

    startCButton.addEventListener('click', toggleAnimationC);

    function toggleAnimationC() {
        if (!animationRunningC) {
            hexElementC.classList.add('animateLine'); // 添加animate类以启动动画
            animationRunningC = true;
        } else {
            hexElementC.classList.remove('animateLine'); // 移除animate类以暂停动画


            hexElementC.style.strokeDashoffsetC = '1000px'; // 将动画重置到起始状态

            // 使用setTimeout在下一帧重新执行动画
            setTimeout(() => {
                hexElementC.classList.add('animateLine'); // 添加animate类以重新启动动画
            }, 0);
        }
    }

    // 身心紓壓 
    const startBButton = document.getElementById('pointBButton');
