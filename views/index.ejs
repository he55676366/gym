            hexElementB.style.strokeDashoffset = '1000px'; // 将动画重置到起始状态

            // 使用setTimeout在下一帧重新执行动画
            setTimeout(() => {
                hexElementB.classList.add('animateLine'); // 添加animate类以重新启动动画
            }, 0);
        }
    }
    
    </script>



