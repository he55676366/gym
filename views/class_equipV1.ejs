					for (var j = 1; j <= 7; j++) {
						var creattd = $('<td>');
						creattd.append(
							`<span>${i}:00</span>-<span>${i + 1}:00</span><p></p>`
						);

						creattd.addClass('spacetd');
						newtr.append(creattd);
					}

					$('.space .schedule').append(newtr);
					continue;
				}
				else {

					for (var j = 1; j <= 7; j++) {
						var creattd = $('<td>');
						creattd.append(
							`<span  name='${j - 1}'>${i}:00</span>-<span>${i + 1}:00</span><p></p>`
						);
